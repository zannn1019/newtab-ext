{
  "manifest_version": 3,
  "name": "Zan",
  "version": "1.0.0",
  "description": "A minimalist Japanese-inspired homepage with live market tickers, zen quotes, and fluid GSAP transitions.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://api.coingecko.com/*"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self' 'unsafe-inline'"
  },
  "action": {
    "default_title": "Zan"
  }
}
